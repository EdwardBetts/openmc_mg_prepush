<?xml version="1.0"?>
<materials>

  <!--
====================================================
Description: Simple thermal system from INDC-USA-107
Case: Problem 1 (1/2" pin, 2" lattice pitch)
Written By: Paul Romano
Changed to MG By: Kendra Keady
Date: 7/1/2013
====================================================
-->

  <default_xs>50m</default_xs>

  <!-- Fuel composed of 99.02 a/o U-238 and 0.98 a/o U-235 -->
  <material id="1">
    <density value="18.8" units="g/cc" />
    <nuclide name="U-238" ao="99.02" />
    <nuclide name="U-235" ao="0.98" />
  </material>

  <!-- Water at 1 g/cc -->
  <material id="2">
    <density value="1.0" units="g/cc" />
    <nuclide name="H-1" ao="2.0" />
    <nuclide name="O-16" ao="1.0" />
  </material>

</materials>
